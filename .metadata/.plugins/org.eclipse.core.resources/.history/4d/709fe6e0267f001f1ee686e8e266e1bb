package com.StepDefinition;

import io.cucumber.java.en.And;
import io.cucumber.java.en.Given;
import io.cucumber.java.en.Then;
import io.cucumber.java.en.When;

public class StepDefinition {

	@Given("user will launch the browser and will pass the {string}")
	public void user_will_launch_the_browser_and_will_pass_the(String string) {
		System.out.println("user is on launch page");
	}

	@When("user will enter the username {string} and password {string}")
	public void user_will_enter_the_username_and_password(String string, String string2) {
		System.out.println("user will enter username and password");
	}

	@And("user will click on login button")
	public void user_will_click_on_login_button() {
		System.out.println("user will press submit button");
	}

	@Then("user will go to the home page")
	public void user_will_go_to_the_home_page() {
		System.out.println("user is on home page");
	}

}
